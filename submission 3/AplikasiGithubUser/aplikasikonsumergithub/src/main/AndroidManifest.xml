<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alifyacode.aplikasikonsumergithub">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.alifyacode.aplikasigithubuser.READ_DATABASE"/>
    <uses-permission android:name="com.alifyacode.aplikasigithubuser.WRITE_DATABASE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/github"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/github"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityFile.UGthbDetailActivity" />
        <activity android:name=".ActivityFile.UGthbMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>